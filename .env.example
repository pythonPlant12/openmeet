# Domain
DOMAIN=openmeet.eu

# TURN Server (CoTURN)
TURN_USER=openmeet
TURN_PASSWORD=secure_password
TURN_REALM=openmeet.eu

# VPS IP (needed for TURN external-ip)
VPS_IP=vps_ip

# SSL Certificate paths (Let's Encrypt)
SSL_CERT_PATH=/etc/letsencrypt/live/openmeet.eu/fullchain.pem
SSL_KEY_PATH=/etc/letsencrypt/live/openmeet.eu/privkey.pem
